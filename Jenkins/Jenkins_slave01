#!groovy
// Ждет окончания предыдущей сборки (нельзя несколько одновременно)
properties([disableConcurrentBuilds()]) 
pipeline {
 agent { label 'slave01' }
 options {
   // Выводить в логе сборки время каждой операции
   timestamps() 
  }
  stages {
       stage('launch slave') {
      steps {
            build 'Ansible_master'
      }
    }
  }
  
}
