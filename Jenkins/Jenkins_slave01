#!groovy
// Ждет окончания предыдущей сборки (нельзя несколько одновременно)
properties([disableConcurrentBuilds()]) 
pipeline {
 agent { label 'slave01' }
 options {
   // Выводить в логе сборки время каждой операции
   timestamps() 
  }
  stages {
       stage('launch slave') {
      steps {
            build 'Dev_master'
            archiveArtifacts artifacts: 'Calculator/target/Calculator-1.0-SNAPSHOT.jar', fingerprint: true
      }
    }
    
  }
  
}
