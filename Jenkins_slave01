#!groovy
// Ждет окончания предыдущей сборки (нельзя несколько одновременно)
properties([disableConcurrentBuilds()]) 
pipeline {
 agent { label 'slave01' }
 options {
   // Выводить в логе сборки время каждой операции
   timestamps() 
  }
  stages {
       stage('launch slave') {
      steps {
        //  waitUntil {
            build 'Ansible'
        //  }
      }
    }
  }
  post { 
        // Выполняет в конце сборки (всегда)
        always { 
            echo  "Ansible + Jenkins"
        }
    }
}
